config_version=5

[application]

config/name="HexRun"
run/main_scene="res://Scenes/main_menu.tscn"
config/features=PackedStringArray("4.1", "GL Compatibility")
run/max_fps=120
boot_splash/bg_color=Color(0.0980392, 0.121569, 0.286275, 1)
boot_splash/image="res://Images/sticker.png"
config/icon="res://Images/Player.png"
config/movie_file=""
config/size/extend_to_title=false
config/size/borderless=false
config/size/mode=0

[autoload]

EndMenu="*res://Scenes/end_menu.tscn"
Global="*res://Global/global.gd"
UI="*res://UI/ui.tscn"
TransitionLayer="*res://Global/transition_layer.tscn"

[display]

window/icon="res://Images/Player.png"
window/frame_delay_msec=0
window/image="res://Images/sticker.png"
window/fullsize=true
window/bg_color=Color(0.0980392, 0.121569, 0.286275, 1)
window/stdout/print_fps=false

[editor]

movie_writer/fps=120
script/physics_ticks_per_second=120
script/physics_jitter_fix=0.5
script/stdout/print_fps=true
script/max_fps=120
script/fps=120

[editor_plugins]

enabled=PackedStringArray("res://addons/godot_vim/plugin.cfg")

